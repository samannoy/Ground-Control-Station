<Window x:Class="MaterialDash.UploadPopup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Upload" Height="300" Width="400">
    <Grid>
        <TextBox Name="InputTextBox" Margin="10" VerticalAlignment="Top" HorizontalAlignment="Stretch" />
        <Button Content="Upload" Name="UploadButton" Width="80" Height="30" HorizontalAlignment="Right" Margin="0,75,10,79" Click="UploadButton_Click"/>
        <Button Content="Cancel" Width="78" Height="30" HorizontalAlignment="Right" Margin="0,75,101,79" Click="Cancel_Button_Click"/>
    </Grid>
</Window>

